{
    "env": {
        "browser": false,
        "node": true
    },
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": false,
        "codeFrame": false
    },
    "rules": {
        "strict": 1,
        "quotes": [2, "single"]
    }
}
